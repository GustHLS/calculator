}

/* Numbers, Negativa and Comma */
#bt0, #bt1, #bt2, #bt3, #bt4, 
#bt5, #bt6, #bt7, #bt8, #bt9,
#bt_negative, #bt_comma {
	background-color: #698092;
	color: white;
}

#bt0::hover, #bt1::hover, #bt2::hover, #bt3::hover, #bt4::hover, 
#bt5::hover, #bt6::hover, #bt7::hover, #bt8::hover, #bt9::hover,
#bt_negative::hover, #bt_comma::hover {
	background-color: #758a97;
	color: white;
}


/* Plus, Minus, Times, Divided, Sqt, Power, Fraction, Factorial, Percentage, Clear e Del */
#bt_plus,#bt_minus, #bt_times, 
#bt_divided, #bt_sqt, #bt_power, #bt_fraction,
#bt_factorial, #bt_percentage, #bt_clear, #bt_del {
	background-color: #90A4B5;
}

#bt_plus::hover,#bt_minus::hover, #bt_times::hover, 
#bt_divided::hover, #bt_sqt::hover, #bt_power::hover, #bt_fraction::hover,
#bt_factorial::hover, #bt_percentage::hover, #bt_clear::hover, #bt_del::hover {
	background-color: #a7b8c7;
}

/* Equal */
#bt_equal {
	background-color: #CD6155;
	color: white;
}

#bt_equal::hover {
	background-color: #cf7368;
	color: white;
}  

